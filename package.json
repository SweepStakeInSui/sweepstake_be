{
    "name": "sweepstake-be",
    "version": "0.0.1",
    "description": "",
    "author": "",
    "private": true,
    "license": "UNLICENSED",
    "scripts": {
        "prebuild": "rimraf dist",
        "build": "yarn prebuild && nest build",
        "format": "prettier --write \"src/**/*.ts\"",
        "start": "yarn prebuild && nest start",
        "start:dev": "yarn prebuild && nest start --watch",
        "start:debug": "yarn prebuild && nest start --debug --watch",
        "start:prod": "node dist/main.js",
        "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix --max-warnings=0",
        "test": "jest --setupFiles dotenv/config",
        "test:watch": "jest --watch --setupFiles dotenv/config",
        "test:cov": "jest --coverage --setupFiles dotenv/config",
        "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
        "test:e2e": "jest --config ./test/jest-e2e.json --setupFiles dotenv/config",
        "typeorm": "node --require ts-node/register ./node_modules/typeorm/cli.js",
        "migration:generate": "yarn build && yarn typeorm migration:generate -- -n",
        "migration:create": "typeorm migration:create -n",
        "migration:run": "typeorm migration:run",
        "migration:revert": "typeorm migration:revert",
        "entity:create": "typeorm entity:create -n",
        "postinstall": "husky",
        "husky": "husky install",
        "generate-typechain": "typechain --target=ethers-v6 --out-dir src/modules/contract/types src/modules/contract/abi/vesting.json",
        "seeder": "ts-node -r tsconfig-paths/register src/console.ts seeding-data",
        "remove-temp-merge-expire": "ts-node -r tsconfig-paths/register src/console.ts remove-temp-merge-expire",
        "check-temp-merge-about-to-succeed": "ts-node -r tsconfig-paths/register src/console.ts check-temp-merge-is-about-to-succeed",
        "console:dev": "ts-node -r tsconfig-paths/register src/console.ts",
        "console": "node dist/src/console.js"
    },
    "lint-staged": {
        "*.ts": [
            "yarn lint",
            "yarn format"
        ]
    },
    "dependencies": {
        "@nestjs/axios": "3.0.2",
        "@nestjs/common": "10.3.10",
        "@nestjs/config": "3.2.3",
        "@nestjs/core": "10.3.10",
        "@nestjs/event-emitter": "2.0.4",
        "@nestjs/jwt": "10.2.0",
        "@nestjs/microservices": "10.3.10",
        "@nestjs/passport": "10.0.3",
        "@nestjs/platform-express": "10.3.10",
        "@nestjs/schedule": "4.0.2",
        "@nestjs/terminus": "10.2.3",
        "@nestjs/typeorm": "10.0.2",
        "@songkeys/nestjs-redis": "10.0.0",
        "axios": "1.7.2",
        "class-transformer": "0.5.1",
        "class-validator": "0.14.1",
        "class-validator-extended": "4.0.0",
        "dayjs": "1.11.11",
        "ethers": "6.13.1",
        "ioredis": "5.4.1",
        "joi": "17.13.3",
        "lodash": "4.17.21",
        "log4js": "6.9.1",
        "morgan": "1.10.0",
        "mysql2": "3.10.2",
        "nestjs-console": "9.0.0",
        "nestjs-typeorm-paginate": "4.0.4",
        "passport": "0.7.0",
        "passport-custom": "^1.1.1",
        "passport-jwt": "^4.0.1",
        "reflect-metadata": "0.2.2",
        "remove": "0.1.5",
        "rimraf": "5.0.7",
        "rxjs": "7.8.1",
        "swagger-ui-express": "5.0.1",
        "typeorm": "0.3.20",
        "uuid": "10.0.0",
        "webpack": "5.92.1"
    },
    "devDependencies": {
        "@commitlint/cli": "19.3.0",
        "@commitlint/config-conventional": "19.2.2",
        "@eslint/eslintrc": "^3.1.0",
        "@eslint/js": "^9.6.0",
        "@golevelup/ts-jest": "0.5.0",
        "@nestjs/cli": "10.4.0",
        "@nestjs/schematics": "10.1.2",
        "@nestjs/serve-static": "4.0.2",
        "@nestjs/swagger": "7.4.0",
        "@nestjs/testing": "10.3.10",
        "@typechain/ethers-v6": "0.5.1",
        "@types/cache-manager": "4.0.6",
        "@types/express": "4.17.21",
        "@types/jest": "29.5.12",
        "@types/lodash": "4.17.6",
        "@types/morgan": "1.9.9",
        "@types/multer": "1.4.11",
        "@types/node": "20.14.9",
        "@types/passport": "1.0.16",
        "@types/passport-jwt": "^4.0.1",
        "@types/uuid": "10.0.0",
        "@typescript-eslint/eslint-plugin": "7.15.0",
        "@typescript-eslint/parser": "7.15.0",
        "eslint": "9.6.0",
        "eslint-config-prettier": "9.1.0",
        "eslint-plugin-prettier": "5.1.3",
        "globals": "^15.8.0",
        "husky": "9.0.11",
        "jest": "29.7.0",
        "jest-extended": "4.0.2",
        "lint-staged": "15.2.7",
        "prettier": "3.3.2",
        "source-map-support": "0.5.21",
        "supertest": "7.0.0",
        "ts-jest": "29.1.5",
        "ts-loader": "9.5.1",
        "ts-node": "10.9.2",
        "tsconfig-paths": "4.2.0",
        "typechain": "8.3.2",
        "typescript": "5.5.3"
    },
    "packageManager": "yarn@4.3.1"
}
